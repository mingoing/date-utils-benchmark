{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["es2020","DOM"],
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "allowJs": true,
    "sourceMap": false,
    "declaration": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "types": ["vitest/globals"],
    "noEmit": true
  },
}
